{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Odoo",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/../odoo-repo/odoo-bin",
            "args": [
                "--addons-path",
                "${workspaceFolder}/../odoo-repo/addons,${workspaceFolder}",
                "-d",
                "rd-demo",
                "--dev",
                "debug",
                "--http-port",
                "8069",
                "--limit-time-real",
                "1000000",
                "-u",
                "estate",
                "--dev",
                 "xml"
            ],
            "cwd": "${workspaceFolder}",
            "env": {},
            "envFile": "${workspaceFolder}/.env"
        }
    ]
}